{
    "name": "escuelait/laravel-api-kit",
    "description": "Utils to create consisten APIs in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "midesweb",
            "email": "malvarez@escuela.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "EscuelaIT\\APIKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EscuelaIT\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EscuelaIT\\APIKit\\ApiKitServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "negartarh/apiwrapper": "^0.9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "orchestra/testbench": "^10.9"
    }
}
