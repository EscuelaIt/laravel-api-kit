{
    "name": "escuelait/laravel-api-kit",
    "description": "Utilities to create consistent APIs in Laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "escuelait",
        "api",
        "resource",
        "kit"
    ],
    "homepage": "https://github.com/EscuelaIt/laravel-api-kit",
    "authors": [
        {
            "name": "midesweb",
            "email": "malvarez@escuela.it",
            "homepage": "https://escuela.it",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "EscuelaIT\\APIKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EscuelaIT\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EscuelaIT\\APIKit\\APIKitServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "negartarh/apiwrapper": "^0.9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "orchestra/testbench": "^10.9",
        "friendsofphp/php-cs-fixer": "^3.93"
    }
}
